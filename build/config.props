<Project ToolsVersion="15.0">
  <!-- Dependency versions -->
  <PropertyGroup>
    <NuGetPackageVersion>4.3.0</NuGetPackageVersion>
    <XunitVersion>2.2.0</XunitVersion>
    <XunitRunnerVisualStudioVersion>$(XunitVersion)</XunitRunnerVisualStudioVersion>
    <XunitAnalyzersVersion>0.6.0</XunitAnalyzersVersion>
    <TestSDKVersion>15.0.0</TestSDKVersion>
    <FluentAssertionsVersion>4.19.3</FluentAssertionsVersion>
    <SourceLinkVersion>2.1.2</SourceLinkVersion>
  </PropertyGroup>

  <!-- Config -->
  <PropertyGroup>
    <RepositoryName>NuGet.Test.Helpers</RepositoryName>
  </PropertyGroup>

  <!-- Nuspec defaults -->
  <PropertyGroup>
    <Authors>emgarten</Authors>
    <ProjectUrl>https://github.com/emgarten/nuget.test.helpers</ProjectUrl>
    <PackageLicenseUrl>https://github.com/emgarten/nuget.test.helpers/blob/master/LICENSE.md</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/emgarten/nuget.test.helpers</RepositoryUrl>
    <PackageTags>nuget</PackageTags>
    <Description>NuGet helpers for creating test nupkgs.</Description>
    <Copyright>Copyright Â© 2017 Justin Emgarten</Copyright>
    <PackageIconUrl>https://emgartenstatic.blob.core.windows.net/nupkgs/icons/nuget.test.helpers.png</PackageIconUrl>
  </PropertyGroup>
</Project>
