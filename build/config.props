<Project ToolsVersion="15.0">
  <!-- Dependency versions -->
  <PropertyGroup>
    <NuGetPackageVersion>6.12.1</NuGetPackageVersion>
  </PropertyGroup>

  <!-- Config -->
  <PropertyGroup>
    <RepositoryName>NuGet.Test.Helpers</RepositoryName>
    <PackageIconPath>$(MSBuildThisFileDirectory)/icons/icon.png</PackageIconPath>
  </PropertyGroup>

  <!-- Nuspec defaults -->
  <PropertyGroup>
    <Authors>emgarten</Authors>
    <PackageProjectUrl>https://github.com/emgarten/nuget.test.helpers</PackageProjectUrl>
    <RepositoryUrl>https://github.com/emgarten/nuget.test.helpers</RepositoryUrl>
    <PackageTags>nuget;nupkgs</PackageTags>
    <Description>A library for creating nupkgs and nuspecs to use in tests.</Description>
  </PropertyGroup>
</Project>
